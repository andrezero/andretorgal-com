---
import LogoSvg from './logo/LogoSvg.astro';
import MusicSvg from './logo/MusicSvg.astro';
import StorySvg from './logo/StorySvg.astro';
import WorkSvg from './logo/WorkSvg.astro';

export interface Props {
    variant?: 'logo' | 'work' | 'story' | 'music';
}

const { variant = 'logo' } = Astro.props;

// const classNames = [{ auto: autoSize }];

const svgMap = {
    logo: LogoSvg,
    music: MusicSvg,
    story: StorySvg,
    work: WorkSvg,
};

// eslint-disable-next-line security/detect-object-injection
const Logo = svgMap[variant];
---

<Logo />
